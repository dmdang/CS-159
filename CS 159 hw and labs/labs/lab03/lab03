#include <stdio.h>

int main()
{
  int x;
  int y;
  int max;

  printf("Enter two integers: ");
  scanf("%d %d", &x, &y);

  max = x / y * x + y / x *y

  printf("Max value input is: %d\n", max);

  return(0);
}

